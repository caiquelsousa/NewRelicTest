{
  "name": "new-relic",
  "version": "1.0.0",
  "description": "New Relic - Interview Test",
  "scripts": {
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.config.js",
    "buildDev": "rm -rf dist && webpack --mode development --config webpack.config.js",
    "start-server": "node ./dist/server.js",
    "start": "export NODE_ENV=production && yarn run buildProd && yarn run start-server",
    "dev": "export NODE_ENV=development && yarn run buildDev && yarn run start-server",
    "test": "jest --coverage"
  },
  "author": "Caique Sousa",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "construct-style-sheets-polyfill": "^2.3.5"
  }
}
